{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "rootDir": "src",
    "lib": ["ES2022"]
  },
  "include": ["src/**/*", "Untitled-1.ts.bak"],
  "exclude": ["node_modules", "dist", "reference", "backups"]
}
