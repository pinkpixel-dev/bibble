{
  "defaultProvider": "openaiCompatible",
  "apis": {
    "openai": {
      "baseUrl": "https://api.openai.com/v1",
      "defaultModel": "gpt-4.1"
    },
    "openaiCompatible": {
      "baseUrl": "https://text.pollinations.ai/openai",
      "defaultModel": "openai",
      "requiresApiKey": false
    },
    "anthropic": {
      "baseUrl": "https://api.anthropic.com",
      "defaultModel": "claude-sonnet-4-20250514"
    },
    "google": {
      "baseUrl": "https://generativelanguage.googleapis.com",
      "defaultModel": "gemini-2.0-flash"
    }
  },
  "ui": {
    "colorOutput": true,
    "useMarkdown": true
  },
  "mcpServers": [
    {
      "name": "desktop-commander",
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ],
      "env": {
        "fileWriteLineLimit": "10000",
        "fileReadLineLimit": "10000"
      },
      "enabled": true
    },
    {
      "name": "context7",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "datetime",
      "command": "npx",
      "args": [
        "-y",
        "@pinkpixel/datetime-mcp"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "mcpollinations",
      "command": "npx",
      "args": [
        "-y",
        "@pinkpixel/mcpollinations"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "sequential-thinking",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "taskflow",
      "command": "npx",
      "args": [
        "-y",
        "@pinkpixel/taskflow-mcp"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "web-scout",
      "command": "npx",
      "args": [
        "-y",
        "@pinkpixel/web-scout-mcp"
      ],
      "env": {},
      "enabled": true
    },
    {
      "name": "notifications",
      "command": "npx",
      "args": [
        "-y",
        "@pinkpixel/notification-mcp"
      ],
      "env": {
        "MCP_NOTIFICATION_SOUND": "random"
      },
      "enabled": true
    },
    {
      "name": "puppeteer",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {},
      "enabled": true
    }
  ],
  "chat": {
    "saveHistory": true,
    "maxHistoryItems": 50,
    "userGuidelines": ""
  },
  "security": {
    "defaultPolicy": "prompt",
    "requireConfirmationGlobally": false,
    "previewToolInputs": true,
    "auditLogging": false,
    "toolTimeout": 30000,
    "sensitiveOperations": [
      "execute_command",
      "force_terminate",
      "kill_process",
      "move_file",
      "delete_memory",
      "delete_task",
      "delete_subtask",
      "delete_note",
      "remove_todos",
      "set_config_value"
    ],
    "serverPolicies": {
      "mcpollinations": "trusted"
    },
    "allowedTools": {
      "desktop-commander": [
        "read_file"
      ]
    },
    "blockedTools": {
      "desktop-commander": [
        "search_code"
      ]
    }
  },
