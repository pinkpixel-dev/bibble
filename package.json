{
  "name": "@pinkpixel/bibble",
  "version": "1.4.5",
  "description": "CLI chatbot application with MCP support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "bibble": "bin/bibble-cli.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --external commander,chalk,inquirer,markdown-it,openai,boxen,uuid,child_process,cross-spawn,@anthropic-ai/sdk,isbinaryfile,zod,zod-to-json-schema",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "postinstall": "node -e \"try { require('fs').chmodSync('./bin/bibble.js', '755'); require('fs').chmodSync('./bin/bibble-cli.js', '755'); require('fs').chmodSync('./bin/bibble-cli.cjs', '755'); } catch (e) { console.error(e) }\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "chatbot",
    "mcp",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "boxen": "^8.0.1",
    "boxen-cli": "^4.0.0",
    "chalk": "^5.6.0",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "cli-truncate": "^4.0.0",
    "clipboardy": "^4.0.0",
    "commander": "^14.0.0",
    "figlet": "^1.8.2",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.6.2",
    "isbinaryfile": "^5.0.6",
    "json-stringify-pretty-compact": "^4.0.0",
    "log-symbols": "^7.0.1",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "meow": "^13.2.0",
    "openai": "^4.103.0",
    "ora": "^8.2.0",
    "string-width": "^8.0.0",
    "strip-ansi": "^7.1.0",
    "supports-color": "^10.2.0",
    "supports-hyperlinks": "^4.1.0",
    "terminal-link": "^4.0.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "wrap-ansi": "^9.0.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
