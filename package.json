{
  "name": "@pinkpixel/bibble",
  "version": "1.8.5",
  "description": "An AI agent/chatbot with built-in tools and MCP support that lives in your terminal",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "bibble": "bin/bibble-cli.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --external commander,chalk,inquirer,markdown-it,openai,boxen,uuid,child_process,cross-spawn,@anthropic-ai/sdk,isbinaryfile,zod,zod-to-json-schema,cli-spinners",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "postinstall": "node -e \"try { require('fs').chmodSync('./bin/bibble.js', '755'); require('fs').chmodSync('./bin/bibble-cli.js', '755'); require('fs').chmodSync('./bin/bibble-cli.cjs', '755'); } catch (e) { console.error(e) }\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "release": "npm run build && npm run lint && npm run typecheck && npm run test && npm run prepare && npm run postinstall && npm publish --access public",
    "prettier": "prettier --write ."
  },
  "keywords": [
    "cli",
    "chatbot",
    "mcp",
    "ai",
    "llm"
  ],
  "author": "Pink Pixel",
  "license": "Apache 2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "ascii-text-generator": "^1.0.1",
    "axios": "^1.11.0",
    "boxen": "^8.0.1",
    "boxen-cli": "^4.0.0",
    "chalk": "^5.6.0",
    "chalk-animation": "^2.0.3",
    "cheerio": "^1.1.2",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^3.2.0",
    "cli-truncate": "^5.0.0",
    "clipboardy": "^4.0.0",
    "commander": "^14.0.0",
    "figlet": "^1.8.2",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.4",
    "isbinaryfile": "^5.0.6",
    "json-stringify-pretty-compact": "^4.0.0",
    "log-symbols": "^7.0.1",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "meow": "^13.2.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.19.1",
    "string-width": "^8.1.0",
    "strip-ansi": "^7.1.0",
    "supports-color": "^10.2.0",
    "supports-hyperlinks": "^4.1.0",
    "terminal-image": "^1.1.0",
    "terminal-link": "^4.0.0",
    "typescript": "^5.9.2",
    "uuid": "^12.0.0",
    "wrap-ansi": "^9.0.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/marked-terminal": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "3.6.2",
    "tsup": "^8.5.0",
    "typescript-eslint": "^8.42.0"
  }
}
